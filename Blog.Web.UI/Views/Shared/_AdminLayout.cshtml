<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>BlogMe | @ViewData["Title"]</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>

    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a asp-controller="AdminHome" asp-action="Index" class="navbar-brand">BlogMe | Dashboard</a>
            </div>
            <ul class="nav navbar-nav">
                <li><a asp-controller="Article" asp-action="Index">Articles</a></li>
                <li><a asp-controller="Author" asp-action="Index">Authors</a></li>
                <li><a asp-controller="Category" asp-action="Index">Categories</a></li>
                <li><a asp-controller="Comment" asp-action="Index">Comments</a></li>
                <li><a asp-controller="Image" asp-action="">Index</a></li>
                <li><a asp-controller="Subscriber" asp-action="Index">Subscribers</a></li>
                <li><a asp-controller="User" asp-action="Index">Users</a></li>
            </ul>
        </div>
    </nav>

    <div class="container">
        @RenderBody()
    </div>
</body>
</html>
